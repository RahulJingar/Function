#include<iostream>
using namespace std;
int createnumber(int number)
{
	int num=0;
	for(int i=0; i<number; i++)
	{
		int digit;
		cout<<"your number is:";
		cin>>digit;
		num=num*10+digit;
	}
	return num;
}
int main()
{
	int num;
	cout<<"enter the number:";
	cin>>num;
	int res=createnumber(num);
	cout<<"your ans is:"<<res<<endl;
}
