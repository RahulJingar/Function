#include<iostream>
using namespace std;
void linearSearch(int arr[],int size)
{
	for(int i=0; i<size; i++)
	{
		for(int j=0; j<size; j++)
		{
			if(i!=j && arr[i]==arr[j])
			{
				cout<<arr[i]<<" ";
			}
		}
	}
}
int main()
{
	int arr[]= {4, 3, 2, 7, 8, 2, 3, 1};
	int size=sizeof(arr)/sizeof(arr[0]);
	linearSearch(arr,size);
}
